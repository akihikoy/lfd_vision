<launch>
  <node type="usb_stereo_node" pkg="lfd_vision" name="usb_stereo" output="screen">
    <param name="camera1" value="1" />
    <param name="camera2" value="2" />
    <param name="cap_width" value="352" />
    <param name="cap_height" value="288" />
    <param name="rotate90n" value="0" />
    <param name="num_detectors" value="2" />
    <param name="block_area_min" value="1.0" />
    <param name="color_files_base1" value="$(find lfd_vision)/x" />
    <param name="color_files_base2" value="$(find lfd_vision)/x2" />
    <param name="img_win" value="1100011" type="str" />

    <param name="ff_ofl_win" value="3" />
    <param name="ff_ofl_spd_min" value="2.0" />
    <param name="ff_er_dl" value="1" />
    <param name="ff_amt_min" value="1.0" />
    <param name="ff_amt_max" value="3000.0" />
    <param name="ff_spd_min" value="1.0" />
    <param name="ff_spd_max" value="-1.0" />
    <param name="ff_mask_flen" value="5" />

    <param name="stereo_param_yaml" value="$(find lfd_vision)/config/ext_usbcam1_stereo3.yaml" />
    <param name="stereo_f_We" value="2" />
    <param name="stereo_f_Wd" value="3" />
    <param name="stereo_f_XFilter" value="1" />
    <param name="stereo_f_YFilter" value="32" />
    <param name="stereo_f_XStep" value="1" />
    <param name="stereo_f_YStep" value="16" />
    <param name="stereo_f_ThMatch" value="16" />
  </node>
</launch>
